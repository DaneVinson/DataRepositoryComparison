<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>
  <appSettings>
    <add key="StorageAccountKey" value="FllBAnlnhTpVN+FBd1j4nq4MbJT75Fno2zEVThSXb0k6GWeXrN8/KHJ0NeheHC+LTTNr19T0+/ybc7I0JKkjFw==" />
    <add key="StorageAccountName" value="danestorage" />
    <add key="StorageTableName" value="Things" />
  </appSettings>
  <log4net>
    <root>
      <level value="ALL" />
      <appender-ref ref="ConsoleAppender" />
      <appender-ref ref="RollingFileAppender" />
    </root>
    <appender name="ConsoleAppender" type="log4net.Appender.ConsoleAppender">
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%message%newline%exception" />
      </layout>
    </appender>
    <appender name="RollingFileAppender" type="log4net.Appender.RollingFileAppender">
      <file value="DataRepositoryComparison.log" />
      <appendToFile value="true" />
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
      <rollingStyle value="Size" />
      <maxSizeRollBackups value="5" />
      <maximumFileSize value="5MB" />
      <staticLogFileName value="true" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date - %-5level - %class - %message%newline%exception" />
      </layout>
    </appender>
  </log4net>

  <startup>
